{{> search }}

<section class="dashboard eightcol">
  <h2>Most-used Gems</h2>

  {{# gems }}
  <div class="gem">
    <div class="count">{{count}}</div>
    <div class="gem"><a href="/gems/{{ name }}">{{ name }}</a></div>
  </div>
  {{/ gems }}
  </ul>
</section>
<aside class="fourcol last">
  <h2>Recent Updates</h2>
  <ul>
    {{# repos }}
    <li><a href="{{ url }}">{{ to_s }}</a></li>
    {{/ repos }}
  </ul>
</aside>
